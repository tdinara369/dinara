	<parameters>
		<defaults>
			<option name="templateColor" value="pink" />
			<option name="logoColored" value="0" />
			<option name="templateWidth">
				<set name="type" value="fixed" />
				<set name="fixed" value="1150" />
			</option>
		</defaults>
		<fieldset name="jsn-layout-size" column="left" label="JSN_TPLFW_SECTION_OVERALL_WIDTH" method="replace">
			<field name="templateWidth" type="jsnwidthtype" default="fixed">
				<option name="fixed" label="JSN_TPLFW_LAYOUT_FIXED" default="1150" suffix="px" />
				<option name="float" label="JSN_TPLFW_LAYOUT_FLOAT" default="90" suffix="%" />
				<option name="responsive" label="JSN_TPLFW_LAYOUT_RESPONSIVE" multiple="1" pro="true">
					<option value="mobile" default="checked">JSN_TPLFW_LAYOUT_RESPONSIVE_WIDTH_MOBILE_DEVICE</option>
					<option value="wide">JSN_TPLFW_LAYOUT_RESPONSIVE_WIDTH_WIDE_DESKTOP</option>
				</option>
			</field>
		</fieldset>
		<fieldset name="jsn-site-tools">
			<field name="sitetoolsColors" type="jsnlistcolor" depends-on="sitetoolStyle" method="replace">
				<option value="pink">JSN_TPLFW_COLOR_PINK</option>
				<option value="red">JSN_TPLFW_COLOR_RED</option>
				<option value="green">JSN_TPLFW_COLOR_GREEN</option>
				<option value="brown">JSN_TPLFW_COLOR_BROWN</option>
				<option value="orange">JSN_TPLFW_COLOR_ORANGE</option>
				<option value="cyan">JSN_TPLFW_COLOR_CYAN</option>
			</field>
		</fieldset>
		<field name="menuSticky" type="jsncheckbox" label="JSN_TPLFW_ENABLE_STICKY_MENU" description="JSN_TPLFW_ENABLE_STICKY_MENU_DESC" method="replace">
				<option value="mobile" default="checked">JSN_TPLFW_ENABLE_STICKY_MENU_ON_MOBILE</option>
				<option value="desktop" default="checked">JSN_TPLFW_ENABLE_STICKY_MENU_ON_DESKTOP</option>
		</field>

	</parameters>
	<config>
		<fields name="params" addfieldpath="/plugins/system/jsntplframework/libraries/joomlashine/elements">
			<fieldset name="message" label="Warning">
				<field name="warning" label="Warning" type="jsnframeworkcheck" />
			</fieldset>
		</fields>
	</config>
</extension>
